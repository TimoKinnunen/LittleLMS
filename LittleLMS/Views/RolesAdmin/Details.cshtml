@model Microsoft.AspNet.Identity.EntityFramework.IdentityRole

@{
    ViewBag.Title = "Rollens detaljer";
}

<h2>Rollens detaljer.</h2>

<div>
    <h4>Roll.</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @*@Html.DisplayNameFor(model => model.Name)*@
            @Html.Label("Rollens namn")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
    </dl>
</div>
<h4>@*List of users in this role*@</h4>
<h4>Lista av användare knutna till denna rollen:</h4>
@if (ViewBag.UserCount == 0)
{
    <hr />
    <p>@*No users found in this role.*@</p>
    <p>Inga användare är knutna till denna rollen.</p>
}

<table class="table">

    @foreach (var item in ViewBag.Users)
    {
        <tr>
            <td>
                @item.UserName
            </td>
        </tr>
    }
</table>
<p>
    @Html.ActionLink("Redigera", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Tillbaka till listan", "Index")
</p>



