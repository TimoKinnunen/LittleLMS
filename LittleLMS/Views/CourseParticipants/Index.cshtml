@model IEnumerable<LittleLMS.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>@ViewBag.WhichCourse</h2>

<h2>Kursdeltagare</h2>

@*<p>
        @Html.ActionLink("Lägg till ny", "Create")
    </p>*@
<h4>Lärare</h4>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.UserName)*@
            @Html.Label("Användarnamn")
        </th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            @*<td>
                    @Html.ActionLink("Redigera", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Detaljer", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Ta bort", "Delete", new { id = item.Id })
                </td>*@
        </tr>
    }
</table>


<h4>Elever</h4>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.FullName)*@
            @Html.Label("Namn")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.UserName)*@
            @Html.Label("Användarnamn")
        </th>
    </tr>
    @foreach (var student in (IEnumerable<LittleLMS.Models.ApplicationUser>)ViewBag.Students) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => student.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => student.UserName)
            </td>
            @*<td>
                    @Html.ActionLink("Redigera", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Detaljer", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Ta bort", "Delete", new { id = item.Id })
                </td>*@
        </tr>
    }
</table>
